digraph "Initialization Stage" {
	node [color=black fillcolor=lightblue fontcolor=black shape=box style=filled]
	fontcolor=black fontsize=20 label="Initialization Stage 

" labeljust=c labelloc=t
	ranksep=1
	A [label="Initialization of the 
 first 2 clusters"]
	B [label="Find boundaries between 
 the initital clusters."]
	C [label="Allocate data points 
 to their 
 corresponding clusters."]
	D [label="Recalculate 
 cluster positionns 
 based on the 
 average."]
	E [label="Find the new boundaries 
 between these 
 recalculated clusters."]
	F [label="Assign data points 
 to their 
 corresponding 
 recalculated clusters."]
	A -> B
	B -> C
	C -> D
	D -> E
	E -> F
}
